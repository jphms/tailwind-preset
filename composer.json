{
    "name": "jphms/tailwind-preset",
    "description": "A Tailwind CSS preset for Laravel.",
    "keywords": ["laravel", "laravel preset", "preset", "tailwind", "tailwindcss"],
    "license": "MIT",
    "authors": [
        {
            "name": "João Magalhães",
            "email": "joao@orgo.ee"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "laravel/framework": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "Orgo\\TailwindPreset\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": ["Orgo\\TailwindPreset\\TailwindServiceProvider"]
        }
    }
}
